<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <solid android:color="#fff" />
            <stroke android:width="0dp" android:color="#0c5f84" />
            <corners android:radius="5dp" />
        </shape>
    </item>
    <item android:bottom="10dp" android:top="2dp" android:right="2dp" android:left="2dp">
        <shape android:shape="rectangle">
            <solid android:color="#000"/>
            <stroke android:width="0.01dp" android:color="#000"/>
        </shape>
    </item>

    <item android:bottom="20dp" android:top="2dp" android:right="2dp" android:left="2dp">
        <shape android:shape="rectangle">
            <solid android:color="#fff"/>
            <stroke android:width="0.01dp" android:color="#000"/>
        </shape>
    </item>
</layer-list>